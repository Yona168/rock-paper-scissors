{
  "compileOnSave": false,
  "include":[
    "src/**/*"
  ],
  "exclude": [
    ".git"
  ],
  "compilerOptions":{
    "baseUrl": "./src",
    "paths": {
      "@util/*": ["util/*"]
    },
    "outDir": "build",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "src",
    "allowSyntheticDefaultImports": true
  }
}
